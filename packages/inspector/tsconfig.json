{
  "extends": "../../tsconfig-base.json",
  "compilerOptions": {
    "outDir": "dist/",
    "rootDir": "./src",
    "declarationDir": "dist",
    "module": "esnext",
    "target": "es5",
    "jsx": "react-jsx",
    "lib": ["dom","ES2019"],
    "strict": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
  },
  "include": ["src"],
  "references": [
    { "path": "../interactor" },
  ]
}
