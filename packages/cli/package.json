{
  "name": "@bigtest/cli",
  "version": "0.8.0",
  "description": "Use BigTest",
  "repository": "https://github.com/thefrontside/bigtest.git",
  "author": "Frontside Engineering <engineering@frontside.com>",
  "main": "dist/src/index.js",
  "license": "MIT",
  "files": [
    "bin/*",
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint '{src,test,bin}/**/*.ts'",
    "test": "mocha -r ts-node/register 'test/{,!(fixtures)/**}/*.test.ts'",
    "mocha": "mocha -r ts-node/register",
    "bigtest": "ts-node src/index.ts",
    "prepack": "tsc --outdir dist --module commonjs && chmod +x dist/bin/bigtest.js"
  },
  "bin": {
    "bigtest": "dist/bin/bigtest.js"
  },
  "devDependencies": {
    "@bigtest/todomvc": "^0.5.3",
    "@frontside/eslint-config": "^1.1.2",
    "@frontside/typescript": "^1.0.1",
    "@effection/channel": "^0.6.6",
    "@effection/events": "^0.7.7",
    "@effection/subscription": "^0.10.0",
    "@types/capture-console": "1.0.0",
    "@types/json5": "^0.0.30",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.13.4",
    "@types/yargs": "^15.0.3",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "eslint-plugin-prefer-let": "^1.0.1",
    "expect": "^24.9.0",
    "mocha": "^6.2.2",
    "ts-node": "*"
  },
  "dependencies": {
    "@bigtest/client": "^0.2.0",
    "@bigtest/effection": "^0.5.3",
