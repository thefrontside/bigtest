    "@bigtest/server": "^0.11.0",
    "@effection/node": "^0.8.0",
    "capture-console": "^1.0.1",
    "deepmerge": "^4.2.2",
    "effection": "^0.7.0",
    "json5": "^2.1.3"
  },
  "volta": {
    "node": "12.16.0",
    "yarn": "1.19.1"
  }
}
