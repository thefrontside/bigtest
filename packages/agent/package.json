{
  "name": "@bigtest/agent",
  "version": "0.5.1",
  "description": "Agent for connecting test environments (browsers) to BigTest",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "repository": "https://github.com/thefrontside/bigtest.git",
  "author": "Frontside Engineering <engineering@frontside.com>",
  "license": "MIT",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint '{app,bin,src,test}/**/*.ts'",
    "test": "mocha -r ts-node/register test/**/*.test.ts",
    "mocha": "mocha -r ts-node/register",
    "start": "parcel serve --out-dir dist/app app/index.html app/harness.ts",
    "prepack": "tsc --outdir dist --declaration --sourcemap && parcel build --no-minify --out-dir dist/app app/index.html app/harness.ts",
    "manifest:build": "parcel build test/fixtures/manifest.src.js --out-dir test/fixtures --out-file manifest.js --global __bigtestManifest"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@bigtest/parcel": "^0.5.1",
    "@bigtest/suite": "^0.5.1",
    "@bigtest/webdriver": "^0.5.1",
    "@frontside/tsconfig": "*",
    "@types/express": "^4.17.2",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.13.4",
    "@types/node-fetch": "^2.5.4",
    "assert": "^2.0.0",
    "classnames": "^2.2.5",
    "expect": "^24.9.0",
    "express": "^4.17.1",
    "mocha": "^6.2.2",
    "node-fetch": "^2.6.0",
    "parcel": "^1.12.4",
    "parcel-bundler": "^1.12.4",
    "regenerator-runtime": "^0.13.3",
    "ts-node": "*",
    "typescript": "^3.7.0"
  },
  "dependencies": {
    "@bigtest/effection": "^0.5.1",
    "@bigtest/effection-express": "^0.5.1",
    "@effection/events": "^0.7.4",
    "bowser": "^2.9.0",
    "effection": "^0.7.0"
  },
  "browserslist": [
    "since 2017-06"
  ],
  "volta": {
    "node": "12.16.0",
    "yarn": "1.19.1"
  }
}
