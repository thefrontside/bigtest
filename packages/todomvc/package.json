{
  "name": "@bigtest/todomvc",
  "version": "0.5.6",
  "description": "Quickly run Todo MVC apps from different frameworks",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/thefrontside/bigtest.git",
  "author": "Frontside Engineering <engineering@frontside.com>",
  "license": "MIT",
  "files": [
    "dist/*",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf dist *.tsbuildinfo",
    "start": "yarn prepack && ts-node dist/start.js",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "mocha -r ts-node/register test/**/*.test.ts",
    "build": "node build.js",
    "prepack": "node build.js"
  },
  "bin": {
    "bigtest-todomvc": "bin/start.js"
  },
  "devDependencies": {
    "@frontside/eslint-config": "^1.1.2",
    "@frontside/typescript": "^1.1.0",
    "@types/express": "^4.17.2",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.13.4",
    "@types/node-fetch": "^2.5.4",
    "classnames": "^2.2.5",
    "expect": "^24.9.0",
    "mkdirp": "^1.0.4",
    "mocha": "^6.2.2",
    "node-fetch": "^2.6.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-scripts": "^3.4.1",
    "todomvc-app-css": "^2.0.6",
    "ts-node": "*",
    "webpack": "4.41.2"
  },
  "dependencies": {
    "@bigtest/effection-express": "^0.9.1",
    "@effection/node": "^0.9.1",
    "effection": "^0.7.0",
    "express": "^4.17.1"
  },
  "volta": {
    "node": "12.16.0",
    "yarn": "1.19.1"
  }
}
