  "scripts": {
    "clean": "rm -rf dist *.tsbuildinfo",
    "start": "ts-node bin/start.ts",
    "query": "ts-node bin/query.ts",
    "query:live": "ts-node bin/live-query.ts",
    "lint": "eslint \"{src,bin,test}/**/*.ts\"",
    "check:types": "tsc --noEmit",
    "test": "mocha ./test/setup test/**/*.test.ts",
    "test:debug": "cross-env LOG_LEVEL=debug yarn test",
    "test:app:start": "node ./bin/app.js",
    "generate:schema": "node generate-schema.js",
    "prepack": "yarn generate:schema && tsc --build ./tsconfig.build.json"
  },
  "files": [
    "README.md",
    "dist/**/*"
  ],
