    "@types/mocha": "^7.0.1",
    "@types/node": "^13.13.4",
    "expect": "^24.9.0",
    "mocha": "^6.2.2",
    "ts-node": "^9.1.1"
  },
  "dependencies": {
    "@effection/channel": "^0.6.8",
    "@effection/events": "^0.7.8",
    "@effection/subscription": "^0.11.0",
    "@frontside/tsconfig": "^1.2.0",
    "assert-ts": "^0.2.2",
    "effection": "0.7.0",
    "fp-ts": "^2.8.2",
    "monocle-ts": "^2.3.3"
  },
  "volta": {
    "node": "12.16.0",
    "yarn": "1.19.1"
  }
}
