{
  "name": "bigtest-sample-app",
  "version": "0.0.0",
  "private": true,
  "description": "BigTest Sample App",
  "repository": "https://github.com/thefrontside/bigtest.git",
  "author": "Frontside Engineering <engineering@frontside.com>",
  "license": "MIT",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html --no-cache",
    "cypress:run": "npx cypress run --spec 'src/test/cypress.spec.js'",
    "test:jest": "jest 'src/test/jest.test.js'",
    "test:cypress": "start-server-and-test 'npm run start -- -p 3000' http://localhost:3000 cypress:run",
    "test:bigtest": "bigtest ci"
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@bigtest/cypress": "^0.0.4",
    "@testing-library/react": "^11.1.0",
    "babel-jest": "^26.6.3",
    "bigtest": "0.13.3",
    "eslint": "^7.17.0",
    "eslint-plugin-cypress": "^2.11.2",
    "jest": "^26.6.3",
    "jest-css-modules-transform": "4.1.0",
    "parcel": "1.12.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-test-renderer": "^17.0.1",
    "start-server-and-test": "^1.11.6",
    "ts-jest": "^26.4.4",
    "typescript": "^4.2.2"
  },
  "resolutions": {
    "@bigtest/agent": "0.17.1-d907a0d7",
    "@bigtest/project": "0.15.1-d907a0d7",
    "@bigtest/server": "0.24.0-d907a0d7"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:cypress/recommended"
    ]
  },
  "browserslist": [
    "defaults",
    "not op_mob > 1"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": true
          }
        }
      ],
      "@babel/preset-react"
    ]
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-babel",
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "jest-css-modules-transform"
    }
  },
  "volta": {
    "node": "12.16.0",
    "yarn": "1.19.1"
  }
}
